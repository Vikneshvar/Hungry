<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.bazz.studios.activity">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme">
        <activity
            android:name=".HomeActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MyInvitesActivity"
            android:label="@string/title_activity_my_invites"></activity>
        <activity
            android:name=".IAmHungryActivity"
            android:label="@string/title_activity_iam_hungry"></activity>
        <activity
            android:name=".MyFriendsActivity"
            android:label="@string/title_activity_my_friends"></activity>
        <activity
            android:name=".LoginActivity"
            android:label="@string/title_activity_login"></activity>
        <activity
            android:name=".RegistrationActivity"
            android:label="@string/title_activity_registration"></activity>
    </application>

</manifest>
